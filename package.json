{
  "name": "gateway",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "scripts": {
    "start": "node --preserve-symlinks --experimental-specifier-resolution=node --enable-source-maps dist/index.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@skyra/http-framework": "^0.0.7",
    "@skyra/shared-http-pieces": "^0.0.7"
  }
}
