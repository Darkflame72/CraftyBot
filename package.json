{
  "name": "gateway",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "scripts": {
    "start": "node --preserve-symlinks --experimental-specifier-resolution=node --enable-source-maps dist/index.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/collection": "^0.5.0",
    "@discordjs/rest": "^0.3.0",
    "@sapphire/pieces": "^3.3.0",
    "discord-api-types": "^0.29.0",
    "fastify": "^3.27.4",
    "tweetnacl": "^1.0.3"
  }
}
