{
  "name": "gateway",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "scripts": {
    "start": "node --preserve-symlinks --experimental-specifier-resolution=node --enable-source-maps .",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.4",
    "@swc/core": "^1.2.164",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "0.13.0-dev.1648515278-ac4bc3a",
    "@skyra/http-framework": "^0.2.2",
    "discord-api-types": "^0.31.0",
    "dotenv": "^16.0.0",
    "tslib": "^2.3.1"
  },
  "main": "dist/index.js"
}
