{
  "name": "gateway",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "scripts": {
    "start": "node --preserve-symlinks --experimental-specifier-resolution=node --enable-source-maps .",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.3",
    "@swc/core": "^1.2.159",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@sapphire/pieces": "^3.3.0",
    "@skyra/http-framework": "^0.0.7",
    "@skyra/shared-http-pieces": "^0.0.7",
    "dotenv": "^16.0.0"
  },
  "main": "dist/index.js"
}
